{
    "version": 2,
    "builds": [
      {
        "src": "server/routes/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "server/mongodb/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/dalle/(.*)",
        "dest": "/server/routes/dalleRoutes.js"
      },
      {
        "src": "/api/post/(.*)",
        "dest": "/server/routes/postRoutes.js"
      }
    ]
  }
  